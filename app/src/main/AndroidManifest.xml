<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.micha.todo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_rhoen_apotheke"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/volleyball2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.micha.todo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.micha.todo.Heute" />
        <activity android:name="com.example.micha.todo.AufgabenListe" />
        <activity android:name="com.example.micha.todo.NeueAufgabe"></activity>
    </application>

</manifest>